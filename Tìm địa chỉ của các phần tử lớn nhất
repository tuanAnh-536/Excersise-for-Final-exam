#include<iostream>
using namespace std;
void max(int *arr,int n){
    int max;
    *arr=max;
    for(int i=0;i<n;i++)
    {
       if(arr[i]>max){
           max=arr[i];
       }
    }
    for(int i=0;i<n;i++){
        if(arr[i]==max){
        cout<<&arr[i]<<endl;
        }
    }
    
}
void nhap(int *arr,int n){
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
}
int main(){
    int n;
    cin>>n;
    int*a=new int[n];
    nhap(a,n);
    max(a,n);
}
