#include<iostream>
using namespace std;
#include <math.h>
void Factorial(int n){
if(n<0){cout<<"Giai thua khong xac dinh cho so am";
       return;}
    long long fact=1;///Tránh tràn bộ nhớ
    for(int i=1;i<=n;i++){
        fact*=i;}
        cout<<fact<<endl;
}
int main(){
    int n;
    cin>>n;
    Factorial(n);
    
}
